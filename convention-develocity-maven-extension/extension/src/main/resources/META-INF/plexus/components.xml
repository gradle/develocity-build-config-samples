<?xml version="1.0" encoding="UTF-8"?>
<component-set>
    <components>
        <component>
            <role>com.gradle.develocity.agent.maven.api.DevelocityListener</role>
            <role-hint>convention-develocity-listener</role-hint>
            <implementation>com.myorg.ConventionDevelocityListener</implementation>
            <description>Configures the Develocity Maven extension for com.myorg</description>
            <isolated-realm>false</isolated-realm>
        </component>
        <component>
            <role>org.apache.maven.AbstractMavenLifecycleParticipant</role>
            <role-hint>openapi-configurer</role-hint>
            <implementation>com.myorg.OpenApiConfigurer</implementation>
            <description>Configures OpenAPI to hide timestamps in Generated annotations</description>
            <isolated-realm>false</isolated-realm>
        </component>
    </components>
</component-set>
